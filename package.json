{
    "name": "@abrahamsaanchez/eslint-config",
    "type": "module",
    "version": "2.0.0",
    "description": "Abraham's ESLint config",
    "author": "Abraham SÃ¡nchez <abrahamsaanchez@gmail.com> (https://github.com/abrahamsaanchez/)",
    "license": "MIT",
    "homepage": "https://github.com/abrahamsaanchez/eslint-config",
    "keywords": [
        "eslint-config"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --clean --dts",
        "commit": "git add . && git-cz",
        "commit:amend": "git add . && git commit --amend --no-edit",
        "dev": "tsup src/index.ts --format esm,cjs --watch & eslint-flat-config-viewer",
        "lint": "bun run typecheck && bun run stub && eslint .",
        "lint:fix": "bun run stub && eslint --fix .",
        "prepack": "nr build",
        "prepare": "husky",
        "release": "bumpp --commit \"chore: Release v%s.\" && bun publish",
        "stub": "tsup src/index.ts --format esm",
        "typecheck": "tsc --noEmit"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    },
    "dependencies": {
        "@angular-eslint/template-parser": "18.4.0",
        "@antfu/eslint-define-config": "1.23.0-2",
        "@stylistic/eslint-plugin": "2.10.1",
        "@typescript-eslint/eslint-plugin": "8.13.0",
        "@typescript-eslint/parser": "8.13.0",
        "eslint-config-flat-gitignore": "0.3.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-import-resolver-typescript": "3.6.3",
        "eslint-plugin-antfu": "2.7.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-import-x": "4.4.0",
        "eslint-plugin-jest": "28.8.3",
        "eslint-plugin-jsdoc": "50.4.3",
        "eslint-plugin-jsonc": "2.16.0",
        "eslint-plugin-markdown": "5.1.0",
        "eslint-plugin-n": "17.12.0",
        "eslint-plugin-no-only-tests": "3.3.0",
        "eslint-plugin-perfectionist": "3.9.1",
        "eslint-plugin-prettier": "5.2.1",
        "eslint-plugin-tailwindcss": "3.17.5",
        "eslint-plugin-unicorn": "56.0.0",
        "eslint-plugin-unused-imports": "4.1.4",
        "eslint-plugin-yml": "1.15.0",
        "globals": "15.12.0",
        "jsonc-eslint-parser": "2.4.0",
        "local-pkg": "0.5.0",
        "yaml-eslint-parser": "1.2.3"
    },
    "devDependencies": {
        "@antfu/eslint-config": "3.8.0",
        "@antfu/ni": "0.23.0",
        "@commitlint/cli": "19.5.0",
        "@commitlint/cz-commitlint": "19.5.0",
        "@stylistic/eslint-plugin-migrate": "2.10.1",
        "@types/eslint": "9.6.1",
        "@types/fs-extra": "11.0.4",
        "@types/jest": "29.5.14",
        "@types/node": "22.9.0",
        "bumpp": "9.8.1",
        "eslint": "9.14.0",
        "eslint-flat-config-viewer": "0.1.20",
        "esno": "4.8.0",
        "fast-glob": "3.3.2",
        "fs-extra": "11.2.0",
        "husky": "9.1.6",
        "jest": "29.7.0",
        "rimraf": "6.0.1",
        "sucrase": "3.35.0",
        "tsup": "8.3.5",
        "typescript": "5.6.3",
        "unbuild": "2.0.0"
    },
    "peerDependencies": {
        "eslint": ">=9.10.0"
    }
}
