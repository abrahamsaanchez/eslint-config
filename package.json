{
    "name": "@abrahamsaanchez/eslint-config",
    "type": "module",
    "version": "1.0.10",
    "description": "Abraham's ESLint config",
    "author": "Abraham SÃ¡nchez <abrahamsaanchez@gmail.com> (https://github.com/abrahamsaanchez/)",
    "license": "MIT",
    "homepage": "https://github.com/abrahamsaanchez/eslint-config",
    "keywords": [
        "eslint-config"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --clean --dts",
        "commit": "git add . && git-cz",
        "commit:amend": "git add . && git commit --amend --no-edit",
        "dev": "tsup src/index.ts --format esm,cjs --watch & eslint-flat-config-viewer",
        "lint": "npm run stub && eslint .",
        "lint:fix": "npm run stub && eslint --fix .",
        "prepack": "nr build",
        "prepare": "husky install",
        "release": "bumpp --commit \"chore: Release v%s.\" && npm publish",
        "stub": "tsup src/index.ts --format esm",
        "typecheck": "tsc --noEmit"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    },
    "dependencies": {
        "@angular-eslint/template-parser": "17.3.0",
        "@antfu/eslint-define-config": "1.23.0-2",
        "@stylistic/eslint-plugin": "1.7.2",
        "@typescript-eslint/eslint-plugin": "7.7.1",
        "@typescript-eslint/parser": "7.7.1",
        "eslint-config-flat-gitignore": "0.1.5",
        "eslint-config-prettier": "9.1.0",
        "eslint-import-resolver-typescript": "3.6.1",
        "eslint-plugin-antfu": "2.1.2",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-i": "2.29.1",
        "eslint-plugin-jest": "28.2.0",
        "eslint-plugin-jsdoc": "48.2.3",
        "eslint-plugin-jsonc": "2.15.1",
        "eslint-plugin-markdown": "4.0.1",
        "eslint-plugin-n": "17.3.1",
        "eslint-plugin-no-only-tests": "3.1.0",
        "eslint-plugin-perfectionist": "2.10.0",
        "eslint-plugin-prettier": "5.1.3",
        "eslint-plugin-tailwindcss": "3.15.1",
        "eslint-plugin-unicorn": "52.0.0",
        "eslint-plugin-unused-imports": "3.1.0",
        "eslint-plugin-yml": "1.14.0",
        "globals": "15.0.0",
        "jsonc-eslint-parser": "2.4.0",
        "local-pkg": "0.5.0",
        "yaml-eslint-parser": "1.2.2"
    },
    "devDependencies": {
        "@antfu/ni": "0.21.12",
        "@commitlint/cli": "19.3.0",
        "@commitlint/cz-commitlint": "19.2.0",
        "@stylistic/eslint-plugin-migrate": "1.7.2",
        "@types/eslint": "8.56.10",
        "@types/fs-extra": "11.0.4",
        "@types/jest": "29.5.12",
        "@types/node": "20.12.7",
        "bumpp": "9.4.0",
        "eslint": "9.1.1",
        "eslint-flat-config-viewer": "0.1.20",
        "esno": "4.7.0",
        "fast-glob": "3.3.2",
        "fs-extra": "11.2.0",
        "husky": "9.0.11",
        "jest": "29.7.0",
        "rimraf": "5.0.5",
        "sucrase": "3.35.0",
        "tsup": "8.0.2",
        "typescript": "5.4.5",
        "unbuild": "2.0.0"
    },
    "peerDependencies": {
        "eslint": ">=8.0.0"
    }
}
